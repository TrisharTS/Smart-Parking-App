<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smartparkingapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/parklogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SmartParkingApp">
        <activity android:name=".Feedback"></activity>
        <activity android:name=".Exit" />
        <activity android:name=".UserDashboard" />
        <activity
            android:name=".ParkingForm"
            android:label="@string/title_activity_parking_form"
            android:theme="@style/Theme.SmartParkingApp.NoActionBar" />
        <activity android:name=".BookSlot" />
        <activity android:name=".Login" />
        <activity android:name=".Signup" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>